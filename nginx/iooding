server {
        listen 80;
        server_name 3.66.51.156;

        location = /favicon.ico { access_log off; log_not_found off; }

        location /static/ {
                root /var/www/iooding;
        }

        location /media/ {
                root /home/ubuntu/iooding;
        }

        location / {
                include proxy_params;
                proxy_pass http://unix:/home/ubuntu/gunicorn.sock;
        }
}